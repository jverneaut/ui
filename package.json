{
  "name": "@studiometa/ui-workspace",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "npm run test --workspace=@studiometa/ui-tests",
    "build": "npm run build:babel && npm run build:types && npm run build:cp-files",
    "build:cp-files": "cp packages/ui/package.json dist/ && cp LICENSE dist/ && cp README.md dist/",
    "build:types": "tsc --build tsconfig.build.json",
    "build:babel": "babel --no-comments --delete-dir-on-start --out-dir dist/ packages/ui",
    "lint": "npm run lint:eslint && npm run lint:types",
    "lint:eslint": "eslint packages/ui",
    "lint:types": "tsc --build tsconfig.lint.json",
    "docs:dev": "npm run dev --workspace=@studiometa/ui-docs",
    "docs:build": "npm run build --workspace=@studiometa/ui-docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@studiometa/eslint-config": "^2.1.3",
    "@studiometa/prettier-config": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.5.0",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@babel/plugin-proposal-async-generator-functions": "^7.15.8",
    "@babel/plugin-syntax-private-property-in-object": "^7.14.5"
  }
}
